	<item id="59800" type="Armor" name="Hero Red Cloak">
		<!-- Description: Cloak that makes you feel Freya's chill. Increases P. Def. and attribute resistance. Unlocks a skill to move to the boss area. Can only be equipped when the cloak slot is open. Enchantment, attribute, and refining are impossible. Can be exchanged or dropped. -->
		<set name="icon" val="fixicon.iconheroredcloak" />
		<set name="default_action" val="EQUIP" />
		<set name="bodypart" val="back" />
		<set name="crystal_type" val="S84" />
		<set name="immediate_effect" val="true" />
		<set name="material" val="CLOTH" />
		<set name="price" val="32971650" />
		<set name="weight" val="220" />
		<set name="item_skill" val="21243-1" />
		<cond msgId="2453">
			<player cloakStatus="true" />
		</cond>
		<for>
			<add stat="darkRes" val="10" />
			<add stat="earthRes" val="10" />
			<add stat="fireRes" val="10" />
			<add stat="holyRes" val="20" />
			<add stat="waterRes" val="20" />
			<add stat="windRes" val="20" />
			<add order="0" stat="pDef" val="30" />
		</for>
	</item>	
